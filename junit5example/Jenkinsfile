pipeline {
    agent any
    stages {
        stage ('comile stage') {
            steps{
                withMaven(maven: 'maven_3_8_4'){
                    sh 'mvn clean comile'
                }
            }

        }
        
        stage ('Testing Stage') {
            steps{
                withMaven(maven: 'maven_3_8_4'){
                    sh 'mvn test'
				}
            }
        }

        stage ('Deploy Stage') {
            steps{
                withMaven(maven: 'maven_3_8_4'){
                    sh 'mvn deploy'
				}
            }
        }
    }
}